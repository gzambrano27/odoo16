    <report id="stock_request_report"
            model="stock.product.request"
            string="Stock Request"
            name="stock_product_request.stock_request_report_content"
            file="stock_product_request.stock_request_report_content"
            report_type="qweb-pdf"
            paperformat="stock_request_format"
            attachment_use="False" />

</odoo>
