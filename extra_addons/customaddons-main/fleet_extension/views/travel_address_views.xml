<odoo>
    <record id="view_travel_address_tree" model="ir.ui.view">
        <field name="name">travel.address.tree</field>
        <field name="model">travel.address</field>
        <field name="arch" type="xml">
            <tree string="Direcciones">
                <field name="name"/>
                <field name="place"/>
            </tree>
        </field>
    </record>

    <record id="view_travel_address_form" model="ir.ui.view">
        <field name="name">travel.address.form</field>
        <field name="model">travel.address</field>
        <field name="arch" type="xml">
            <form string="Direcciones">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="place"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
