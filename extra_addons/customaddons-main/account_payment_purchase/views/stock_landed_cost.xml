<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_stock_landed_cost_form_inhe" model="ir.ui.view">
    <field name="name">stock.landed.cost.new</field>
    <field name="model">stock.landed.cost</field>
    <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form"/>
    <field name="arch" type="xml">
      <field name="account_move_id" position="after">
          <field name="class_id"/>
      </field>
    </field>
   </record>
   
   <record id="account_move_line_inhe" model="ir.ui.view">
      <field name="name">account.move.inhe.tree</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_move_form"/>
      <field name="arch" type="xml">
	      <xpath expr="//field[@name='line_ids']/tree/field[@name='partner_id']" position="after">
	          <field name="class_id" options="{'no_quick_create': True, 'no_create_edit' : True}"/>
	        </xpath>
      </field>
    </record>
</odoo>

