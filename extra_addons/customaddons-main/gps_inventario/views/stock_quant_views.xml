<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_quant_tree_editable_warehouse" model="ir.ui.view">
        <field name="name">stock.quant.tree.editable.warehouse</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable" />
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="warehouse_id" optional="hidden"/>
            </field>
            <field name="product_id" position="after">
                <field name="product_code" optional="hidden"/>
                <field name="product_name" optional="hidden"/>
                <field name="value_float" readonly="1" optional="hidden" />
            </field>
        </field>
	</record>

</odoo>