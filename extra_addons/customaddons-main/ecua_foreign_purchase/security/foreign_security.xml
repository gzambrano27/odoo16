<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record model="ir.module.category" id="module_category_ecua_foreign_purchase">
        <field name="name">Foreign Purchase</field>
        <field name="description">Importaciones</field>
        <field name="sequence">99</field>
    </record>
    <record id="foreign_group_user" model="res.groups">
        <field name="name">Usuario</field>
        <field name="category_id" ref="ecua_foreign_purchase.module_category_ecua_foreign_purchase"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

    <record id="foreign_group_manager" model="res.groups">
        <field name="name">Responsable</field>
        <field name="category_id" ref="ecua_foreign_purchase.module_category_ecua_foreign_purchase"/>
        <field name="implied_ids" eval="[(4, ref('foreign_group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>


</odoo>