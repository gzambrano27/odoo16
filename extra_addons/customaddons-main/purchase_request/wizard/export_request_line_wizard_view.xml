<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_purchase_request_line_export_wizard" model="ir.ui.view">
        <field name="name">purchase.request.line.export.wizard.form</field>
        <field name="model">purchase.request.line.export.wizard</field>
        <field name="arch" type="xml">
            <form string="Exportar Requisiciones">
                <group>
                    <field name="file_name" readonly="1"/>
                    <field name="file_data" filename="file_name"/>
                </group>
                <footer>
                    <button string="Cerrar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
