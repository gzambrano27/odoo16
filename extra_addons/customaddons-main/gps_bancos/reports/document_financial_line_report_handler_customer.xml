                        <field name="subformula">total</field>
                    </record>
                </field>

            </record>


        </field>


        </record>

     <record id="action_document_financial_line_report_cxc" model="ir.actions.client">
            <field name="name">Acuerdos de Cobros &amp; Vencidos por Cobrar</field>
            <field name="tag">account_report</field>
            <field name="context" eval="{'report_id': ref('gps_bancos.document_financial_line_report_c')}"/>
        </record>

</odoo>