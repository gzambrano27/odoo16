<?xml version="1.0" encoding='UTF-8'?>
<odoo>
 <data noupdate="1">

	<record model="ir.cron" id="send_mails_queue_bank">
        <field name="name">Enviar Notificaciones Pagos al Banco</field>
        <field name="interval_number">5</field>
        <field name="sequence">900</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
        <field name="model_id" ref="model_bank_mail_message"/>
        <field name="state">code</field>
        <field name="code">model.send_all_pendings(5,'id desc',[])</field>
  <field name="user_id" ref="base.user_root" />
    </record>


       <record id="ir_cron_send_payment_notifications" model="ir.cron">
            <field name="name">Enviar notificaciones de pagos programados</field>
            <field name="model_id" ref="model_account_configuration_payment"/>
            <field name="state">code</field>
            <field name="code">model.cron_send_payment_notifications(scheduled_hours=[8])</field>
            <field name="active" eval="True"/>
            <field name="interval_number" eval="1"/>
            <field name="interval_type">hours</field>
            <field name="numbercall" eval="-1"/>
            <field name="doall" eval="False"/>
        </record>



 </data>
</odoo>