<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_request_accepted_date_wizard" model="ir.ui.view">
        <field name="name">view.request.accepted.date.wizard</field>
        <field name="model">request.accepted.date.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Fecha de Generacion de Requisicion">
                <group>
                    <group>
                        <field name="date_accept" string='Fecha'/>
                    </group>
                    <group />
                </group>
                <footer>
                    <button
                        name="button_accept"
                        string="Accept"
                        type="object"
                        class="oe_highlight"
                    />
                    <button special="cancel" string="Cancel" class="oe_link" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
