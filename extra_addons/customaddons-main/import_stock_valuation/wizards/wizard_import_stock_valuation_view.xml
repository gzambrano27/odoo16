<odoo>
  <record id="wizard_import_stock_valuation_form" model="ir.ui.view">
    <field name="name">wizard.import.stock.valuation.form</field>
    <field name="model">wizard.import.stock.valuation</field>
    <field name="arch" type="xml">
      <form string="Importar Stock Valuation Layer">
        <group>
          <field name="file" filename="filename"/>
          <field name="filename"/>
        </group>
        <footer>
          <button string="Importar" type="object" name="action_import" class="btn-primary"/>
          <button string="Cancelar" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>

  <record id="action_open_wizard_import_stock_valuation" model="ir.actions.act_window">
    <field name="name">Importar Stock Valuation</field>
    <field name="res_model">wizard.import.stock.valuation</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
  </record>

  <menuitem id="menu_import_stock_valuation"
            name="Importar Valorizaciones"
            parent="stock.menu_stock_warehouse_mgmt"
            action="action_open_wizard_import_stock_valuation"/>
</odoo>