<?xml version="1.0" ?>
<odoo>



<record id="view_account_report_form_inherit_presupuesto" model="ir.ui.view">
    <field name="name">account.report.form.inherit.presupuesto</field>
    <field name="model">account.report</field>
    <field name="inherit_id" ref="account_reports.account_report_form"/>
    <field name="arch" type="xml">

        <!-- Insertar el grupo después del último grupo dentro de la pestaña "Options" -->
        <xpath expr="//page[.//field[@name='filter_show_draft']]/group" position="after">
            <group string="Personalización" class="oe_edit_only">
                <field name="report_action_id"
                colspan="2"
                       options="{'no_create':True,'no_edit':True,'no_open':True}"

                />
            </group>
            <group >

            </group>
        </xpath>

    </field>
</record>








</odoo>