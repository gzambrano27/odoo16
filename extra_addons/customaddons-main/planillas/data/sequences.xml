<odoo>
    <data noupdate="1">
        <!-- Secuencia para Secciones -->
        <record id="seq_planillas_seccion" model="ir.sequence">
            <field name="name">Secuencia para Secciones</field>
            <field name="code">planillas.seccion</field>
            <field name="prefix">SEC-</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Secuencia para Categorías -->
        <record id="seq_planillas_categoria" model="ir.sequence">
            <field name="name">Secuencia para Categorías</field>
            <field name="code">planillas.categoria</field>
            <field name="prefix">CAT-</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Secuencia para Rubros -->
        <record id="seq_planillas_rubro" model="ir.sequence">
            <field name="name">Secuencia para Rubros</field>
            <field name="code">planillas.rubro</field>
            <field name="prefix">RUB-</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Secuencia para Planillas -->
        <record id="seq_planillas_planilla" model="ir.sequence">
            <field name="name">Secuencia para Planillas</field>
            <field name="code">planillas.planilla</field>
            <field name="prefix">PLANILLA-</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
